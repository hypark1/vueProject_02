onst item = {\n          name: payload.name,\n          role: payload.role,\n          accessToken: payload.accessToken\n        }\n        localStorage.setItem('cdpUser', JSON.stringify(item))\n        commit('setUser', item)\n      } else {\n        localStorage.removeItem('cdpUser')\n        commit('setError', 'fail')\n      }\n    },\n    logout ({ commit }) {\n      localStorage.removeItem('cdpUser')\n    }\n  }\n}\n"]}]}